@model UpdateServiceDto
@{
    ViewData["Title"] = "AddStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post" >
    <input type="hidden" asp-for="ServiceID" />
    <label>Hizmet İkonu</label>
    <input class="form-control" type="text" asp-for="ServiceIcon" />
    <span asp-validation-for="ServiceIcon" class="text-info" ></span>
    <br />
     <label>Hizmet Başlığı</label>
    <input class="form-control" type="text" asp-for="Title" />
       <span asp-validation-for="Title" class="text-danger" ></span>
    <br />
     <label>Açıklama</label>
    <input class="form-control" type="text" asp-for="Description" />
       <span asp-validation-for="Description" class="text-danger" ></span>
   <br />
    <button class="btn btn-primary" >Hizmeti Güncelle</button>
</form>

